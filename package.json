{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sunrise-sunset-times",
  "title": "Sunrise / Sunset Times",
  "description": "See sunrise / sunset times for dates up to a month in the future.",
  "icon": "icon.png",
  "author": "jeffreydyao",
  "categories": [
    "Other",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Check Sunrise / Sunset Times",
      "subtitle": "Up to a month in the future",
      "description": "See sunrise / sunset times for dates up to a month in the future.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "dayjs": "^1.11.0",
    "node-fetch": "^3.2.3",
    "suncalc": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/suncalc": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
